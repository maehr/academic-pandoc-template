  date = {2015-01-30},
  pages = {1-32},
  keywords = {act_NetworkAnalysis,act_Theorizing,obj_ANTHROPOLOGY,obj_Archaeology,obj_Network science,obj_Relational archaeology},
  author = {Collar, Anna and Coward, Fiona and Brughmans, Tom and Mills, Barbara J.}
}

